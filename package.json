{
  "name": "@droplink/cache-decorators",
  "version": "1.3.0",
  "description": "A library for caching with decorators",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/droplinkme/cache-decorators.git"
  },
  "author": "Allan C Vieira <allancnfx.vieira@gmail.com>",
  "license": "MIT",
  "keywords": [
    "cache",
    "decorators",
    "caching",
    "redis"
  ],
  "main": "./built/index.js",
  "module": "./built/index.mjs",
  "types": "./built/index.d.ts",
  "scripts": {
    "build": "rm -rf ./built && tsc --project tsconfig.build.json",
    "prepare": "yarn build && cp package.json built",
    "test": "jest --coverage=false",
    "test:cov": "jest --coverage=true",
    "commit": "cz",
    "release": "standard-version",
    "semantic-release": "npx semantic-release"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "dependencies": {
    "ioredis": "^5.3.2",
    "reflect-metadata": "^0.2.1"
  },
  "devDependencies": {
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/commit-analyzer": "^11.1.0",
    "@semantic-release/exec": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^10.0.2",
    "@semantic-release/npm": "^11.0.3",
    "@semantic-release/release-notes-generator": "^12.1.0",
    "@types/jest": "^29.5.12",
    "@types/module-alias": "^2.0.4",
    "@types/node": "^20.11.26",
    "@typescript-eslint/eslint-plugin": "^7.2.0",
    "@typescript-eslint/parser": "^7.2.0",
    "commitizen": "^4.3.0",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "jest": "^29.7.0",
    "prettier": "^3.2.5",
    "semantic-release": "^23.0.2",
    "standard-version": "^9.5.0",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.4.2"
  },
  "publishConfig": {
    "access": "restricted",
    "tag": "latest"
  },
  "files": [
    "built/"
  ],
  "_moduleAliases": {
    "@core": "./built/src/core/*",
    "@database": "./built/src/database/*"
  }
}
