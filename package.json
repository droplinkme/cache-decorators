{
  "name": "@droplink/cache-decorators",
  "version": "1.8.3",
  "description": "A library for caching with decorators",
  "author": "Allan C Vieira <allancnfx.vieira@gmail.com>",
  "license": "MIT",
  "type": "module",
  "keywords": [
    "typescript",
    "cache",
    "decorators",
    "decorator",
    "caching",
    "ts",
    "redis",
    "ioredis"
  ],
  "main": "./built/index.js",
  "module": "./built/index.mjs",
  "types": "./built/index.d.ts",
  "scripts": {
    "build": "rm -rf ./built && swc src index.ts fake.ts -d built && tsc --emitDeclarationOnly --outDir ./built",
    "prepare": "yarn build && cp package.json built",
    "test": "vitest --config vitest.config.unit.ts",
    "test:cov": "vitest --coverage --config vitest.config.unit.ts",
    "test:e2e": "vitest --config vitest.config.e2e.ts",
    "test:watch": "vitest --watch --clearScreen --config vitest.config.unit.ts",
    "test:e2e:cov": "jest --coverage=true --config=jest.config.e2e.js",
    "commit": "cz",
    "release": "standard-version",
    "semantic-release": "npx semantic-release"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "dependencies": {
    "reflect-metadata": "^0.2.1"
  },
  "devDependencies": {
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/commit-analyzer": "^12.0.0",
    "@semantic-release/exec": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^10.0.2",
    "@semantic-release/npm": "^12.0.0",
    "@semantic-release/release-notes-generator": "^12.1.0",
    "@swc/cli": "^0.3.12",
    "@swc/core": "^1.4.13",
    "@types/jest": "^29.5.12",
    "@types/module-alias": "^2.0.4",
    "@types/node": "^20.11.30",
    "@typescript-eslint/eslint-plugin": "^7.3.1",
    "@typescript-eslint/parser": "^7.3.1",
    "@vitest/coverage-v8": "^1.4.0",
    "commitizen": "^4.3.0",
    "cz-conventional-changelog": "^3.3.0",
    "dotenv": "^16.4.5",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "ioredis": "^5.3.2",
    "jest": "^29.7.0",
    "prettier": "^3.2.5",
    "semantic-release": "^23.0.5",
    "standard-version": "^9.5.0",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "tsc-alias": "^1.8.8",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.4.2",
    "unplugin-swc": "^1.4.5",
    "vitest": "^1.4.0"
  },
  "publishConfig": {
    "access": "public",
    "tag": "latest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/droplinkme/cache-decorators.git"
  },
  "homepage": "https://github.com/droplinkme/cache-decorators/blob/main/README.md",
  "bugs": {
    "url": "https://github.com/droplinkme/cache-decorators/issues"
  },
  "files": [
    "built/"
  ],
  "_moduleAliases": {
    "@core": "./built/src/core/*",
    "@database": "./built/src/database/*"
  }
}
